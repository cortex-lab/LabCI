  "bugs": {
    "url": "https://github.com/cortex-lab/matlab-ci/issues"
  },
  "homepage": "https://github.com/cortex-lab/matlab-ci#readme",
  "dependencies": {
    "@octokit/app": "^4.2.1",
    "@octokit/request": "^5.4.9",
    "express": "^4.17.1",
    "localtunnel": "^2.0.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "fake-timers": "^0.1.2",
    "mocha": "^8.2.0",
    "nock": "^13.0.4",
    "shelljs": "^0.8.4",
    "sinon": "^9.2.1",
