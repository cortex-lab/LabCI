{
  "name": "matlab-ci",
  "version": "1.0.0",
  "description": "A small set of modules written in Node.js for running automated tests of MATLAB code in response to GitHub events.  Also submits code coverage to the Coveralls API.",
  "main": "main.js",
  "scripts": {
    "test": "env NODE_ENV=test mocha ./test --recursive --reporter spec",
    "start": "node -r dotenv/config main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cortex-lab/matlab-ci.git"
  },
  "keywords": [],
